{
  "name": "twaese",
  "version": "1.0.0",
  "description": "Text mangler optimized for tweets",
  "scripts": {
    "lint:js": "eslint --ext js,vue,json src/*",
    "lint:ec": "eclint check $(git ls-files)",
    "lint": "npm run lint:js && npm run lint:ec",
    "test": "npm run lint",
    "dev": "NODE_ENV=development webpack serve --open --hot --mode development",
    "build": "NODE_ENV=production webpack --progress --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freaktechnik/twaese.git"
  },
  "author": "Martin Giger (https://humanoids.be)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freaktechnik/twaese/issues"
  },
  "homepage": "https://github.com/freaktechnik/twaese#readme",
  "private": true,
  "dependencies": {
    "semantic-ui-vue": "^0.11.0",
    "twitter-text": "^3.1.0",
    "vue": "^2.7.15"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.26.0",
    "@freaktechnik/eslint-config-babel": "^9.2.1",
    "@freaktechnik/eslint-config-vue": "^9.2.1",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.1",
    "eclint": "^2.8.1",
    "eslint": "^9.15.0",
