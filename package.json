    "mini-css-extract-plugin": "^2.9.2",
    "semantic-ui-css": "^2.5.0",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "Firefox ESR"
  ]
