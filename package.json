  "dependencies": {
    "semantic-ui-vue": "^0.11.0",
    "twitter-text": "^3.1.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@freaktechnik/eslint-config-babel": "^8.0.2",
    "@freaktechnik/eslint-config-vue": "^8.0.2",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eclint": "^2.8.1",
